! ! @U.VAR.YES@dim-:@U.VAR.YES@chu- VarNclass ;

 LEXICON SetB !possession for the alienable nouns, verbal nouns and tv incompletive
 1sg:ɨn- SetBFunction ;
! ! ! @U.Pref.NO@ TVroots ;

 LEXICON SetBFunction
! ! !  @U.NOM.NO@.incomp-:@U.NOM.NO@0 TVroots ;
! ! ! ! @U.NOM.YES@.poss-:@U.NOM.YES@0 TVroots ;
! ! ! ! @U.NOM.YES@.poss-:@U.NOM.YES@0 IVclass ;
.poss-:0 AlNclass ; ! Add possessive function of prefix to alienable nouns

LEXICON IalNclass 
         IalN ;
         ProperNouns ;

LEXICON IalN
nuuru Num;

LEXICON ProperNouns
hwan # ;

LEXICON AlNclass
@U.ALIEN.YES@  Countnouns ;
@U.ALIEN.YES@  Massnouns ;

LEXICON Countnouns
huurak Num ;

LEXICON Massnouns
 uy # ;

LEXICON VarNclass
@U.ALIEN.YES@ SetB ;
@U.ALIEN.NO@ VarN ;

LEXICON VarN
mɨn'a Num ;

LEXICON Num
.sg:0 SetE ;
-pl:-lh%{F%} SetE ;




